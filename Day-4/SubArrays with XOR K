//Method 1---O(n^2)

#include <bits/stdc++.h> 
int subarraysXor(vector<int> &arr, int x)
{
    int n=arr.size();
    int count=0;
    for(int i=0;i<n;i++){
        long long xork=0;
        for(int j=i;j<n;j++){
            xork^=arr[j];
            if(xork==x){
                count++;
            }
        }
    }
    return count;
}
